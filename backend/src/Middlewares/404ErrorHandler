const ApiError = require("../Utils/ApiError")

const NotFoundError=(err,req,res,next)=>{
    const error_obj={
        statusCode:500,
        msg:err.message,
        stack:err.stack
    }
    if(err instanceof ApiError){
        error_obj.statusCode=err.statusCode
    }
    res.status(error_obj.statusCode).send(error_obj)
}
module.exports=NotFoundError